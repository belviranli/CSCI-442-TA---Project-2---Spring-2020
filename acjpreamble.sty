% Easier cosine with parenthesis
\let\oldcos\cos
\renewcommand{\cos}[1]{\oldcos \hspace{-0.15em} \left( #1 \right)}

% Easier sine with parenthesis
\let\oldsin\sin
\renewcommand{\sin}[1]{\oldsin \hspace{-0.15em} \left( #1 \right)}

% Easier arctangent with parenthesis
\let\oldarctan\arctan
\renewcommand{\arctan}[2]{\oldarctan \hspace{-0.15em} \left( \frac{#1}{#2} \right)}

% Matrix variable with a pre-superscript and subscript
\newcommand{\matrx}[3]{\prescript{#1}{}{\bm{\mathrm{#2}}_{#3}}}

% Unit vector notation
\newcommand{\uvec}[1]{\boldsymbol{\hat{\textbf{#1}}}}

% Easier Full Sized Fractions
\let\oldfrac\frac{}{}
\renewcommand{\frac}[2]{\displaystyle \oldfrac{#1}{#2}}

% Matrix representation of a rotation about the z-axis
\newcommand{\rotz}[1]{
\begin{bmatrix}
\cos{#1}        &&      -\sin{#1}       &&      0 \\
\sin{#1}        &&      \sin{#1}        &&      0 \\
0               &&      0               &&      1 \\
\end{bmatrix}}

% Matrix representation of a rotation about the y-axis
\newcommand{\roty}[1]{
\begin{bmatrix}
\cos{#1}        &&      0       &&      \sin{#1} \\
0               &&      1       &&      0 \\
-\sin{#1}       &&      0       &&      \cos{#1} \\
\end{bmatrix}}

% Matrix representation of a rotation about the x-axis
\newcommand{\rotx}[1]{
\begin{bmatrix}
1       &&      0               &&      0 \\
0       &&      \cos{#1}        &&      -\sin{#1} \\
0       &&      \sin{#1}        &&      \cos{#1} \\
\end{bmatrix}}

% Probability
\newcommand{\prob}[1]{\textrm{P} \hspace{-0.15em} \left( #1 \right)}

% Easier cosine and sine shorthand

\newcommand{\co}[0]{c_{1}}
\newcommand{\ct}[0]{c_{2}}
\newcommand{\cth}[0]{c_{3}}

\newcommand{\ctth}[0]{c_{23}}

\newcommand{\so}[0]{s_{1}}
\newcommand{\st}[0]{s_{2}}
\newcommand{\sth}[0]{s_{3}}

\newcommand{\stth}[0]{s_{23}}

% CCW Arrow for Summing Moments

\newcommand\CircArrowLeft[1]{\stackengine{-.3ex}{#1}{\CAL}{O}{c}{F}{F}{L}}
\newcommand\CAL{\scalebox{2}{\rotatebox[origin=center]{90}{$\circlearrowleft$}}}

